		"editorBracketMatch.background": "#f0f5fa",
		"editorBracketMatch.border": "#e8e8e8",
		"editorCodeLens.foreground": "#0366d6",
		"editorCursor.background": "#33434B",
		"editorCursor.foreground": "#33434B",
		"editorError.border": "#ffffff",
		"editorError.foreground": "#d21d00",
		"editorGroup.border": "#dddddd",
		"editorGroupHeader.tabsBackground": "#f3f3f3",
		"editorGutter.addedBackground": "#a1df8a",
		"editorGutter.deletedBackground": "#eb5368",
		"editorGutter.modifiedBackground": "#f0c36f",
		"editorHoverWidget.background": "#ffffff",
		"editorHoverWidget.border": "#dddddd",
		"editorIndentGuide.background": "#ececec7b",
		"editorInfo.border": "#ffffff",
		"editorInfo.foreground": "#deb800",
		"editorLineNumber.foreground": "#b3b3b3",
		"editorLink.activeForeground": "#0366d6",
		"editorOverviewRuler.addedForeground": "#a1df8a",
		"editorOverviewRuler.border": "#dddddd",
		"editorOverviewRuler.deletedForeground": "#eb5368",
		"editorOverviewRuler.errorForeground": "#d21d00",
		"editorOverviewRuler.infoForeground": "#deb800",
		"editorOverviewRuler.modifiedForeground": "#5f7387",
		"editorOverviewRuler.warningForeground": "#deb800",
		"editorRuler.foreground": "#e8e8e8",
		"editorSuggestWidget.background": "#ffffff",
		"editorSuggestWidget.border": "#dddddd",
		"editorSuggestWidget.foreground": "#33434B",
		"editorSuggestWidget.highlightForeground": "#3d82c6",
		"editorSuggestWidget.selectedBackground": "#acd5ff",
		"editorWarning.border": "#ffffff",
		"editorWarning.foreground": "#deb800",
		"editorWhitespace.foreground": "#969896",
		"editorWidget.background": "#ffffff",
		"editorWidget.border": "#dddddd",
		"extensionButton.prominentBackground": "#3d82c6",
		"extensionButton.prominentForeground": "#ffffff",
		"extensionButton.prominentHoverBackground": "#064e93",
		"focusBorder": "#dddddd00",
		"foreground": "#33434B",
		"gitDecoration.conflictingResourceForeground": "#f66a0a",
		"gitDecoration.deletedResourceForeground": "#d21d00",
		"gitDecoration.ignoredResourceForeground": "#2f363d",
		"gitDecoration.modifiedResourceForeground": "#3d82c6",
		"gitDecoration.untrackedResourceForeground": "#444d56",
		"input.background": "#ffffff",
		"input.border": "#dddddd",
		"input.foreground": "#33434B",
		"input.placeholderForeground": "#dddddd",
		"inputOption.activeBorder": "#dddddd",
		"inputValidation.errorBackground": "#ffffff",
		"inputValidation.errorBorder": "#ff3333",
		"inputValidation.infoBackground": "#ffffff",
		"inputValidation.infoBorder": "#2296B6",
		"inputValidation.warningBackground": "#ffffff",
		"inputValidation.warningBorder": "#f0c36f",
		"list.activeSelectionBackground": "#3d82c6",
		"list.activeSelectionForeground": "#ffffff",
		"list.focusBackground": "#dbebfd",
		"list.focusForeground": "#33434B",
		"list.focusHighlightForeground": "#ffffff",
		"list.highlightForeground": "#3d82c6",
		"list.hoverBackground": "#ffffff",
		"list.hoverForeground": "#000000",
		"list.inactiveSelectionBackground": "#dbebfd",
		"list.inactiveSelectionForeground": "#4180c6",
		"merge.border": "#ffffff",
		"merge.currentHeaderBackground": "#f66a0a",
		"panel.background": "#f7f7f7",
		"panel.border": "#dddddd",
		"panelTitle.activeBorder": "#dddddd",
		"panelTitle.activeForeground": "#33434B",
		"panelTitle.inactiveForeground": "#666666",
		"progressBar.background": "#3d82c6",
		"scrollbarSlider.activeBackground": "#cfcfcfee",
		"scrollbarSlider.background": "#cfcfcf52",
		"scrollbarSlider.hoverBackground": "#cfcfcf9e",
		"selection.background": "#3d81c683",
		"sideBar.background": "#f5f5f5",
		"sideBar.border": "#dddddd",
		"sideBar.foreground": "#33434B",
		"sideBarSectionHeader.background": "#ffffff",
		"sideBarSectionHeader.foreground": "#33434B",
		"sideBarSectionHeader.border": "#dddddd",
		"sideBarTitle.foreground": "#33434B",
		"statusBar.background": "#f3f3f3",
		"statusBar.border": "#dddddd",
		"statusBar.debuggingBackground": "#f7f7f7",
		"statusBar.debuggingForeground": "#dddddd",
		"statusBar.foreground": "#4b4b4b",
		"statusBar.noFolderBackground": "#f7f7f7",
		"statusBarItem.activeBackground": "#ffffffc0",
		"statusBarItem.hoverBackground": "#ffffff90",
		"statusBarItem.prominentBackground": "#f7f7f7f7",
		"statusBarItem.prominentHoverBackground": "#dddddd",
		"tab.activeBackground": "#ffffff",
		"tab.activeBorder": "#ffffff",
		"tab.activeForeground": "#666666",
		"tab.border": "#ffffff",
		"tab.inactiveBackground": "#f5f5f5",
		"tab.inactiveForeground": "#666666",
		"tab.hoverBackground": "#ffffff90",
		"tab.unfocusedActiveBackground": "#ffffff",
		"tab.unfocusedActiveBorder": "#ffffff",
		"tab.unfocusedActiveForeground": "#666666",
		"tab.unfocusedInactiveForeground": "#666666",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#85e7fa",
		"terminal.ansiBrightBlack": "#686868",
		"terminal.ansiBrightBlue": "#6871ff",
		"terminal.ansiBrightCyan": "#85e7ee",
		"terminal.ansiBrightGreen": "#cbe645",
		"terminal.ansiBrightMagenta": "#ff77ff",
		"terminal.ansiBrightRed": "#f07178",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBrightYellow": "#ffd864",
		"terminal.ansiCyan": "#85e7ee",
		"terminal.ansiGreen": "#56fa48",
		"terminal.ansiMagenta": "#ae90e9",
		"terminal.ansiRed": "#ff6c69",
		"terminal.ansiWhite": "#c7c7c7",
		"terminal.ansiYellow": "#ffd864",
		"terminal.background": "#142638",
		"terminal.foreground": "#ffffff",
		"widget.shadow": "#dddddd"
	},
	"tokenColors": [
		{
			"scope": [
				"keyword.other.important",
				"markup.bold",
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.italic",
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": [
				"entity.name.namespace",
				"meta.class",
				"meta.separator",
				//"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.comparison",
				"keyword.operator.arithmetic",
				"keyword.operator.bitwise",
				"keyword.operator.assignment",
				"punctuation.section.scope",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"support.function.core",
				"variable.other",
				"variable.parameter",
				"variable.other.constant",
				"variable.other.block",
				"variable.other.readwrite.instance",

				// CSS
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#33434B"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"markup.inline.raw.string.markdown" //markdown inline code
			],
			"settings": {
				"foreground": "#969896"
			}
		},
		{
			"scope": [
				"token.error-token",
				"string.other.link"
			],
			"settings": {
				"foreground": "#E03939"
			}
		},
		{
			"scope": [
				"token.info-token",
				"keyword.control.directive",
				"keyword.preprocessor",
				"entity.name.function.preprocessor",
				"entity.name.variable.preprocessor",
				"markup.deleted",
			],
			"settings": {
				"foreground": "#CA272F"
			}
		},
		{
			"scope": [
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.type.class",
				"entity.other.inherited-class",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#2296B6"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.symbol",
				"constant.language.symbol",
				"keyword.other.unit",
				"keyword.other.operator.overload.cpp",
				"keyword.other.operator.custom-literal.cpp"
			],
			"settings": {
				"foreground": "#078B8B"
			}
		},
		{
			"scope": [
				"variable.other.enummember",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#336363",
			}
		},
		{
			"scope": [
				"constant",
				"constant.language",
				"constant.character.escape",
			],
			"settings": {
				"foreground": "#6D54EB",
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.operator.wordlike",
				"keyword.operator.functionlike",
				"storage",
				"meta.selector",

				// Language-specific keywords
				"keyword.operator.logical.python",
				"variable.language.special.self.python",
				"variable.parameter.function.language.special.self.python",
				"variable.language.this.cpp",

				// JSON, CSS
				"support.type.property-name",
				"support.type.vendored.property-name",

				// HTML
				"entity.name.tag",

				// Markdown
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#9C33B1"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"entity.other.attribute-name",
				"entity.other.attribute-name.id",
				"meta.function-call.generic",
				"keyword.other.special-method",
				"punctuation.definition.entity",
				//"meta.link",
				"meta.require",
				"string.regexp",
				"support.function",				
				"support.function.any-method",
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html",
				
				// CSS
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css",
				"constant.character.entity",
				"punctuation.definition.entity",

				// Markdown
				"markup.italic",
				"markup.bold",
				"markup.strikethrough",
				"punctuation.definition.italic",
				"punctuation.definition.bold",
				"punctuation.definition.strikethrough"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"markup.quote",
				"markup.inserted"
			],
			"settings": {
				"foreground": "#208C47"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#F0C36F"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	],
	"semanticTokenColors": {
		"namespace": "#33434B", //identifiers that declare or reference a namespace, module, or package
		"class": "#2296B6", //identifiers that declare or reference a class type
		"enum": "#2296B6", //identifiers that declare or reference an enumeration type
		"interface": "#2296B6", //identifiers that declare or reference an interface type
		"struct": "#2296B6", //identifiers that declare or reference a struct type
		"typeParameter": "#2296B6", //identifiers that declare or reference a type parameter
		"type": "#2296B6", //identifiers that declare or reference a type that is not covered above
		"module": "#2296B6",
		"parameter": "#33434B", //identifiers that declare or reference a function or method parameters
		"variable": "#33434B", //identifiers that declare or reference a local or global variable
		"property": "#33434B", //identifiers that declare or reference a member property, member field, or member variable
		"enumMember": "#336363", //identifiers that declare or reference an enumeration property, constant, or member
		//"decorator": "#FF0000", //identifiers that declare or reference decorators and annotations
		//"event": "#FF0000", //identifiers that declare an event property
		"function": "#AB6526", //identifiers that declare a function
		"method": "#AB6526", //identifiers that declare a member function or method
		"macro": "#CA272F", //identifiers that declare a macro
		//"label": "#FF0000", //identifiers that declare a label
		"comment": "#969896", //tokens that represent a comment
		"string": "#208c48", //tokens that represent a string literal
		"keyword": "#316BCD", //tokens that represent a language keyword
		"controlKeyword": "#9C33B1",
		"preprocessorKeyword": "#CA272F",
		"selfParameter": "#316BCD",
		"clsParameter": "#316BCD",
		"number": "#078B8B", //tokens that represent a number literal
		"builtinConstant": "#6D54EB",
		//"regexp": "#FF0000", //tokens that represent a regular expression literal
		"operator": "#33434B", //tokens that represent an operator
		"operatorOverload": "#078B8B",
		"memberOperatorOverload": "#078B8B",
		"newOperator": "#316BCD", //new/delete operator tokens
	}
}
